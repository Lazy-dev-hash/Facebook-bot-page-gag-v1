
# Facebook Page Access Token (Required)
PAGE_ACCESS_TOKEN=your_facebook_page_access_token_here

# Verify Token for Webhook Verification (Required)
VERIFY_TOKEN=your_verify_token_here

# Admin User ID for Update Commands (Required - Your Facebook User ID as STRING)
# To find your Facebook User ID, send a message to your bot and check the logs
# Example: ADMIN_USER_ID=1234567890123456
ADMIN_USER_ID=your_facebook_user_id_here

# Optional: Log Level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Port (defaults to 1337)
PORT=1337

# Optional: Repl URL for auto-uptime (auto-detected if not set)
# Example: REPL_URL=https://your-repl-name.your-username.repl.co
REPL_URL=your_repl_url_here
